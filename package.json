{
  "name": "@diffblue/cover-client",
  "version": "0.1.1",
  "description": "Diffblue Cover client library - Node.js API for Diffblue Cover",
  "main": "build/index.js",
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:diffblue/cover-client-js.git"
  },
  "files": [
    "/build",
    "/npm-shrinkwrap.json"
  ],
  "author": "Diffblue Limited",
  "bugs": {
    "url": "https://github.com/diffblue/cover-client-js/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/diffblue/cover-client-js#readme",
  "scripts": {
    "audit-dependencies": "auditjs --prod --noNode",
    "build": "tsc --project tsconfig.json",
    "copyright-check": "ts-node src/scripts/copyrightChecker.ts",
    "coverage": "rm -rf coverage && NODE_DEBUG=nyc nyc --check-coverage --lines 80 mocha --require ts-node/register 'tests/unit/**/*.ts'",
    "create-release": "ts-node src/scripts/createRelease.ts",
    "changelog": "ts-node src/utils/changelog.ts",
    "create-post-release-pull-request": "ts-node src/scripts/createPostReleasePullRequest.ts",
    "create-release-tag": "ts-node src/scripts/createReleaseTag.ts",
    "license-check": "ts-node src/scripts/licenseChecker.ts",
    "lint-ts": "tslint --config tslint.json --format verbose --project tsconfig.json '{src,tests}/**/*.ts' -e 'src/types/external/**'",
    "lint-markdown": "markdownlint 'docs/**/*.md' '*.md'",
    "publish-package": "ts-node src/scripts/publishPackage.ts",
    "test": "mocha --require ts-node/register 'tests/unit/**/*.ts'",
    "integration-test": "mocha --require ts-node/register 'tests/integration/**/*.ts'"
  },
  "dependencies": {
    "@diffblue/java-combiner": "0.0.11",
    "axios": "0.19.0",
    "form-data": "2.3.3",
    "lodash": "4.17.11",
    "mkdirp": "0.5.1",
    "url-join": "4.0.0"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/form-data": "2.2.1",
    "@types/inquirer": "^6.0.1",
    "@types/lodash": "4.14.123",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.6",
    "@types/multer": "1.3.7",
    "@types/node": "11.13.4",
    "@types/semver": "6.0.0",
    "@types/sinon": "7.0.11",
    "auditjs": "3.1.1",
    "bluebird": "3.5.4",
    "express": "4.17.1",
    "glob-gitignore": "1.0.14",
    "http-status-codes": "1.3.2",
    "inquirer": "6.3.1",
    "markdownlint-cli": "0.15.0",
    "mocha": "6.1.3",
    "multer": "1.4.1",
    "nyc": "14.1.0",
    "parse-git": "^1.1.2",
    "semver": "^6.0.0",
    "simple-git": "^1.110.0",
    "sinon": "7.3.1",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.4.3",
    "typescript-tslint-plugin": "0.3.1"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "node_modules/**",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  }
}
