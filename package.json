{
  "name": "diffblue-cover-client-js",
  "version": "0.0.1",
  "description": "Diffblue Cover client library - Node.js API for Diffblue Cover",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:diffblue/cover-client-js.git"
  },
  "author": "Diffblue Limited",
  "bugs": {
    "url": "https://github.com/diffblue/cover-client-js/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/diffblue/cover-client-js#readme",
  "scripts": {
    "copyright-check": "ts-node src/scripts/copyrightChecker.ts",
    "coverage": "rm -rf coverage && NODE_DEBUG=nyc nyc --check-coverage --lines 100 mocha --require ts-node/register 'tests/unit/**/*.ts'",
    "license-check": "ts-node src/scripts/licenseChecker.ts",
    "lint-ts": "tslint --config tslint.json --format verbose --project tsconfig.json 'src/**/*.ts' -e 'src/types/**'",
    "test": "mocha --require ts-node/register 'tests/unit/**/*.ts'"
  },
  "dependencies": {
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@types/lodash": "4.14.123",
    "@types/mocha": "5.2.6",
    "@types/node": "11.13.4",
    "@types/sinon": "7.0.11",
    "bluebird": "3.5.4",
    "glob-gitignore": "1.0.14",
    "mocha": "6.1.3",
    "nyc": "13.3.0",
    "sinon": "7.3.1",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.4.3"
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "node_modules/**",
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "all": true,
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  }
}
